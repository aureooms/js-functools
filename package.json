{
  "bugs": {
    "url": "https://github.com/aureooms/js-functools/issues"
  },
  "dependencies": {},
  "homepage": "http://aureooms.github.io/js-functools/",
  "version": "2.0.3",
  "keywords": [
    "bricks",
    "compose",
    "curry",
    "ender",
    "function",
    "functools",
    "functional",
    "javascript",
    "js",
    "partial"
  ],
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "test": "ava",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "cover": "nyc --reporter=lcov npm test",
    "release": "np"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.5",
    "@babel/register": "7.9.0",
    "ava": "3.8.1",
    "coveralls": "3.0.11",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.2.1",
    "nyc": "15.0.1"
  },
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "description": "functions code bricks for JavaScript",
  "repository": {
    "url": "https://github.com/aureooms/js-functools.git",
    "type": "git"
  },
  "author": "aureooms",
  "name": "@aureooms/js-functools",
  "spm": {
    "main": "js/dist/functools.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-async-generator-functions"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "files": [
    "lib"
  ]
}
